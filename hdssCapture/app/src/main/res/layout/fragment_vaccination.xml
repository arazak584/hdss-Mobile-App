            android:text="@={vaccination.hod}"
            android:inputType="number" />

        <TextView
            android:textColor="@color/black"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/hom" />

        <EditText
            android:id="@+id/hom"
            android:layout_width="match_parent"
