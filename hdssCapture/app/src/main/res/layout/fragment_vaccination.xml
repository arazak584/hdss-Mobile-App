            android:textColor="#000301"
            android:padding="15dp"/>



        <TextView
            goneUnless="@{vaccination.sbf==2}"
            android:textColor="@color/black"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/stm" />

        <EditText
