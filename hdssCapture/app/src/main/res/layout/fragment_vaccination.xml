                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                selection="@{vaccination.admission}"
                android:onItemSelected="@{vaccination::setAdmission}"
                android:gravity="left"
                android:textColor="#000301"
                android:padding="15dp"/>

        <TextView
            goneUnless="@{vaccination.admission==1}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:textColor="@color/black"
            android:text="@string/rea"/>

        <Spinner
            goneUnless="@{vaccination.admission==1}"
            android:id="@+id/rea"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            selection="@{vaccination.rea}"
            android:onItemSelected="@{vaccination::setRea}"
            android:gravity="left"
            android:textColor="#000301"
            android:padding="15dp"/>

            <com.google.android.material.textfield.TextInputLayout
                goneUnless="@{vaccination.rea==5}"
                style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:padding="5dp"
