<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="vaccination"
            type="org.openhds.hdsscapture.entity.Vaccination" />

        <variable
            name="individual"
            type="org.openhds.hdsscapture.entity.Individual" />

        <import type="android.view.View" />

        <variable
            name="eventname"
            type="String" />

    </data>

    <LinearLayout
        android:id="@+id/VAC_LAYOUT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:context=".fragment.VaccinationFragment">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="10dp"
        android:text="Vaccination"
        android:textColor="@color/colorPrimary"
        android:textSize="30sp"
        android:textStyle="bold" />

    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_margin="5dp"
        android:background="@color/colorPrimaryDark" />


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.google.android.material.textfield.TextInputLayout
            android:visibility="gone"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/fw"
            android:text="@string/fw">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/individual_fw"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={vaccination.fw_uuid}"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textPersonName"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:visibility="gone"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/ind_uuid"
            android:text="@string/ind_uuid">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/individual_uuid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={vaccination.individual_uuid}"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textPersonName"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:visibility="gone"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/location_uuid"
            android:text="@string/location_uuid">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/location_uuid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={vaccination.location_uuid}"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textPersonName"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:visibility="gone"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/socialgroupid"
            android:text="@string/socialgroupid">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/socialgroup_uuid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={vaccination.socialgroup_uuid}"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textPersonName"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:visibility="visible"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/actual_dob"
            android:text="@string/actual_dob">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/dob"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={vaccination.dob}"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textPersonName"/>
        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/month" />

        <EditText
            android:id="@+id/month_age"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:ems="10"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:inputType="number" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/week" />

        <EditText
            android:id="@+id/week_age"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:ems="10"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:inputType="number" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:textColor="@color/black"
            android:text="@string/hcard"/>

        <Spinner
            android:id="@+id/hcard"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            selection="@{vaccination.hcard}"
            android:onItemSelected="@{vaccination::setHcard}"
            android:gravity="left"
            android:textColor="#000301"
            android:padding="15dp"/>

        <LinearLayout
            goneUnless="@{vaccination.hcard==1}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:textColor="@color/black"
            android:text="@string/vareason"/>

        <Spinner
            android:id="@+id/reason"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            selection="@{vaccination.reason}"
            android:onItemSelected="@{vaccination::setReason}"
            android:gravity="left"
            android:textColor="#000301"
            android:padding="15dp"/>

            <com.google.android.material.textfield.TextInputLayout
                goneUnless="@{vaccination.reason==7}"
                style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:padding="5dp"
                android:hint="@string/attend_you_other"
                android:text="@string/attend_you_other">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/reason_oth"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@={vaccination.reason_oth}"
                    android:inputType="textCapCharacters"/>
            </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:textColor="@color/black"
            android:text="@string/onet"/>

        <Spinner
            android:id="@+id/onet"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            selection="@{vaccination.onet}"
            android:onItemSelected="@{vaccination::setOnet}"
            android:gravity="left"
            android:textColor="#000301"
            android:padding="15dp"/>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="start"
                android:padding="5dp"
                android:textColor="@color/black"
                android:text="@string/admission"/>

            <Spinner
