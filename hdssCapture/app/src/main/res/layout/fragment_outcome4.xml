<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="pregoutcome4"
            type="org.openhds.hdsscapture.entity.Outcome" />

    </data>

    <LinearLayout
        android:id="@+id/OUTCOME_LAYOUT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="10dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="10dp"
            android:text="Child-4"
            android:textColor="@color/colorPrimary"
            android:textSize="30sp"
            android:textStyle="bold" />

        <com.google.android.material.textfield.TextInputLayout
            android:visibility="gone"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/uuid"
            android:text="@string/uuid">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/out4_uuid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={pregoutcome4.uuid}"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textPersonName"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:visibility="gone"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/preguuid"
            android:text="@string/preguuid">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/preg4_uuid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={pregoutcome4.preg_uuid}"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textPersonName"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:visibility="gone"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/motherid"
            android:text="@string/motherid">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/out4_preguuid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={pregoutcome4.mother_uuid}"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textPersonName"/>
        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:textColor="@color/black"
            android:text="@string/pregoutcome"/>

        <Spinner
            android:id="@+id/out4_type"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            selection="@{pregoutcome4.type}"
            android:onItemSelected="@{pregoutcome4::setType}"
            android:gravity="left"
            android:textColor="#000301"
            android:padding="15dp"/>


        <com.google.android.material.textfield.TextInputLayout
            android:visibility="gone"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/childextid"
            android:text="@string/childextid">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/out4_child_extid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={pregoutcome4.childuuid}"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textPersonName"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            goneUnless="@{pregoutcome4.type==1}"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:inputType="text"
            android:hint="@string/ind_extid"
            android:text="@string/ind_extid">

            <com.google.android.material.textfield.TextInputEditText
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:id="@+id/individual1_extid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={pregoutcome4.extId}"
                android:inputType="textPersonName"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:visibility="gone"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/motherid"
            android:text="@string/motherid">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/individual1_mother"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={pregoutcome4.mother_uuid}"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textPersonName|textCapCharacters"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            goneUnless="@{pregoutcome4.type==1}"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/firstname"
            android:text="@string/firstname">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/individual1_firstName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={pregoutcome4.firstName}"
                android:required="true"
                android:inputType="textPersonName|textCapCharacters"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            goneUnless="@{pregoutcome4.type==1}"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/lastname"
            android:text="@string/lastname">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/individual1_lastName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={pregoutcome4.lastName}"
                android:required="true"
                android:inputType="textPersonName|textCapCharacters"/>
        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            goneUnless="@{pregoutcome4.type==1}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:textColor="@color/black"
            android:text="@string/gender"/>

        <Spinner
            goneUnless="@{pregoutcome4.type==1}"
            android:id="@+id/gender"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onItemSelected="@{pregoutcome4::setGenders}"
            selection="@{pregoutcome4.gender}"
            android:gravity="left"
            android:textColor="#000301"
            android:required="true"
            android:padding="10dp"/>

        <TextView
            goneUnless="@{pregoutcome4.type==1}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:text="@string/rltn_head"/>

        <Spinner
            goneUnless="@{pregoutcome4.type==1}"
            android:id="@+id/rltn_head"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            selection="@{pregoutcome4.rltn_head}"
            android:onItemSelected="@{pregoutcome4::setRltn_head}"
            android:textColor="#000301"
            android:padding="10dp"/>

    </LinearLayout>

</layout>