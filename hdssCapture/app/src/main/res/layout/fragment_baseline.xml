<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:bind="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="individual"
            type="org.openhds.hdsscapture.entity.Individual" />

        <variable
            name="residency"
            type="org.openhds.hdsscapture.entity.Residency" />

        <variable
            name="mother"
            type="org.openhds.hdsscapture.entity.Individual" />

        <variable
            name="father"
            type="org.openhds.hdsscapture.entity.Individual" />

        <variable
            name="demographic"
            type="org.openhds.hdsscapture.entity.Demographic" />

        <import type="android.view.View" />

        <variable
            name="eventname"
            type="String" />

    </data>

    <LinearLayout
        android:id="@+id/BASELINE_LAYOUT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        tools:context=".Baseline.BaselineFragment">



        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_margin="5dp"
            android:background="@color/colorPrimaryDark" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="10dp"
                android:orientation="vertical">

                <com.google.android.material.textfield.TextInputLayout
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/fw"
                    android:text="@string/fw">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/individual_fw"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={individual.fw_uuid}"
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/ind_uuid"
                    android:text="@string/ind_uuid">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/individual_uuid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={individual.uuid}"
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>

                <Button
                    android:id="@+id/button_individual_insertDate"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:text="@string/visitDate"
                    tools:ignore="DuplicateSpeakableTextCheck"
                    app:icon="@drawable/ic_baseline_calendar_month_24"
                    android:layout_gravity="center"
                    style="@style/SupervisorButton"/>

                <TextView
                    android:id="@+id/individual_insertDate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:autofillHints=""
                    android:text="@={individual.insertDate}"
                    android:ems="10"
                    android:required="true"
                    android:minHeight="30dp"
                    android:textSize="20sp"
                    tools:ignore="TextContrastCheck" />


                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:inputType="text"
                    android:hint="@string/ind_extid"
                    android:text="@string/ind_extid">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/individual_extid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={individual.extId}"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/firstname"
                    android:text="@string/firstname">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/individual_firstName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={individual.firstName}"
                        android:required="true"
                        android:inputType="textPersonName|textCapCharacters"/>
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/lastname"
                    android:text="@string/lastname">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/individual_lastName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={individual.lastName}"
                        android:required="true"
                        android:inputType="textPersonName|textCapCharacters"/>
                </com.google.android.material.textfield.TextInputLayout>

                <LinearLayout
                    android:id="@+id/OTHER_LAYOUT"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:padding="5dp"
                        android:textColor="@color/black"
                        android:text="@string/other_name"/>

                    <Spinner
                        android:id="@+id/other"
                        selection="@{individual.other}"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:contentDescription="@string/other_specify"
                        android:gravity="left"
                        android:minHeight="48dp"
                        android:onItemSelected="@{individual::setOther}"
                        android:padding="15dp"
                        android:spinnerMode="dialog" />

                    <TextView
                        goneUnless="@{individual.other==1}"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:text="@string/ind_nick" />

                    <EditText
                        android:id="@+id/individual_nickName"
                        goneUnless="@{individual.other==1}"
                        android:layout_width="match_parent"
                        android:layout_height="50dp"
                        android:ems="10"
                        android:hint="@string/other_name"
                        android:inputType="textCapCharacters"
                        android:text="@={individual.otherName}" />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:padding="5dp"
                        android:textColor="@color/black"
                        android:text="@string/gh"/>

                    <Spinner
                        android:id="@+id/gh"
                        selection="@{individual.gh}"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:contentDescription="@string/gh"
                        android:gravity="left"
                        android:minHeight="48dp"
                        android:onItemSelected="@{individual::setGh}"
                        android:padding="15dp"
                        android:spinnerMode="dialog" />

                    <com.google.android.material.textfield.TextInputLayout
                        goneUnless="@{individual.gh==1}"
                            android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:padding="5dp"
                        android:hint="@string/ghanacard"
                        android:text="@string/ghanacard">

                        <androidx.appcompat.widget.AppCompatEditText
                            android:id="@+id/ghanacard"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@={individual.ghanacard}"
                            android:inputType="textPersonName|textCapCharacters"/>
                    </com.google.android.material.textfield.TextInputLayout>

                </LinearLayout>

                <Button
                    android:id="@+id/button_individual_Dob"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:text="@string/actual_dob"
                    tools:ignore="DuplicateSpeakableTextCheck"
                    style="@style/SupervisorButton"
                    android:layout_gravity="center"
                    app:icon="@drawable/ic_baseline_calendar_month_24" />

                <TextView
                    android:id="@+id/dob"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@={individual.dob}"
                    android:autofillHints=""
                    android:ems="10"
                    android:minHeight="30dp"
                    android:required="true"
                    android:textSize="20sp"
                    tools:ignore="TextContrastCheck" />

                <EditText
                    android:id="@+id/individ_age"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:ems="10"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:hint="@string/age"
                    android:inputType="number" />


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/dob"/>

                <Spinner
                    android:id="@+id/dobAspect"
                    selection="@{individual.dobAspect}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/dob"
                    android:gravity="left"
                    android:minHeight="48dp"
                    android:onItemSelected="@{individual::setDobAspects}"
                    android:padding="10dp"
                    android:required="true"
                    android:spinnerMode="dialog" />



                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/gender"/>

                <Spinner
                    android:id="@+id/gender"
                    selection="@{individual.gender}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:contentDescription="@string/gender"
                    android:gravity="left"
                    android:minHeight="48dp"
                    android:onItemSelected="@{individual::setGenders}"
                    android:padding="10dp"
                    android:required="true"
                    android:spinnerMode="dialog" />


                <Button
                    android:id="@+id/button_individual_mother"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:text="@string/mother_extid"
                    tools:ignore="DuplicateSpeakableTextCheck"
                    style="@style/SupervisorButton"
                    android:layout_gravity="center"
                    app:icon="@drawable/ic_baseline_pregnant_woman_24" />

                <com.google.android.material.textfield.TextInputLayout
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/motherid"
                    android:text="@string/motherid">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/individual_mother"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={individual.mother_uuid}"
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:inputType="textPersonName|textCapCharacters"/>
                </com.google.android.material.textfield.TextInputLayout>

                <include
                    android:id="@+id/mothers"
                    layout="@layout/fragment_individual_mother"
                    bind:mother="@{mother}" />

                <EditText
                    android:visibility="gone"
                    android:id="@+id/mother_age"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:ems="10"
                    android:focusable="false"
                    android:text="@{individual.m_age}"
                    android:focusableInTouchMode="false"
                    android:inputType="number" />


                <Button
                    android:id="@+id/button_individual_father"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:text="@string/father_extid"
                    style="@style/SupervisorButton"
                    android:layout_gravity="center"
                    tools:ignore="DuplicateSpeakableTextCheck"
                    app:icon="@drawable/ic_baseline_person_24" />

                <com.google.android.material.textfield.TextInputLayout
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/fatherid"
                    android:text="@string/fatherid">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/individual_father"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={individual.father_uuid}"
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:inputType="textPersonName|textCapCharacters"/>
                </com.google.android.material.textfield.TextInputLayout>

                <include
                    android:id="@+id/fathers"
                    layout="@layout/fragment_individual_father"
                    bind:father="@{father}" />

                <EditText
                    android:visibility="gone"
                    android:id="@+id/father_age"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:ems="10"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:text="@{individual.f_age}"
                    android:inputType="number" />


                <com.google.android.material.textfield.TextInputLayout
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/uuid"
                    android:text="@string/uuid">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/uuid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={residency.uuid}"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/uuid"
                    android:text="@string/uuid">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/ind_uuid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={residency.individual_uuid}"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>


                <com.google.android.material.textfield.TextInputLayout
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/location_select"
                    android:text="@string/location_select">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/location_extid"
                        android:text="@={residency.location_uuid}"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>

                <Button
                    android:id="@+id/button_change_hh"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:text="@string/changehh"
                    app:icon="@drawable/baseline_people_24"
                    style="@style/SupervisorButton"
                    android:layout_gravity="center"
                    tools:ignore="DuplicateSpeakableTextCheck" />

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/household"
                    android:text="@string/household">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:id="@+id/socialgroup_hohID"
                        android:text="@={residency.hohID}"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/socialgroupid"
                    android:text="@string/socialgroupid">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/socialgroup_extid"
                        android:text="@={residency.socialgroup_uuid}"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:text="@string/starttype"/>

                <Spinner
                    android:id="@+id/starttype"
                    selection="@{residency.startType}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:minHeight="48dp"
                    android:onItemSelected="@{residency::setStartType}"
                    android:padding="10dp"
                    android:spinnerMode="dialog" />

                <Button
                    android:id="@+id/button_residency_startDate"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:text="@string/startdate"
                    tools:ignore="DuplicateSpeakableTextCheck"
                    style="@style/SupervisorButton"
                    android:layout_gravity="center"
                    app:icon="@drawable/ic_baseline_calendar_month_24" />

                <TextView
                    android:id="@+id/editText_startDate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:autofillHints=""
                    android:ems="10"
                    android:minHeight="40dp"
                    android:text="@={residency.startDate}"
                    android:textSize="20sp"
                    tools:ignore="TextContrastCheck" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:text="@string/endtype"/>

                <Spinner
                    android:id="@+id/endtype"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    selection="@{residency.endType}"
                    android:onItemSelected="@{residency::setEndType}"
                    android:gravity="left"
                    android:spinnerMode="dialog"
                    android:padding="10dp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:text="@string/rltn_head"/>

                <Spinner
                    android:id="@+id/rltn_head"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    selection="@{residency.rltn_head}"
                    android:onItemSelected="@{residency::setRltn_head}"
                    android:spinnerMode="dialog"
                    android:padding="10dp"/>

            <!--                Demographic-->

                <TextView
                    android:id="@+id/yn_phone"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/own_phone"/>

                <org.openhds.hdsscapture.SearchableSpinner
                    android:visibility="gone"
                    android:id="@+id/demo_phone"
                    selection="@{demographic.phone}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:minHeight="48dp"
                    android:onItemSelected="@{demographic::setPhone}"
                    android:padding="15dp"
                    android:spinnerMode="dialog"
                    android:focusable="true"
                    android:textColor="#000301" />

                <com.google.android.material.textfield.TextInputLayout
                    goneUnless="@{demographic.phone==1}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/phone"
                    android:text="@string/phone">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/phone1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={demographic.phone1}"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/tribe"/>

                <org.openhds.hdsscapture.SearchableSpinner
                    android:id="@+id/tribe"
                    selection="@{demographic.tribe}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:minHeight="48dp"
                    android:onItemSelected="@{demographic::setTribes}"
                    android:padding="10dp" />


                <TextView
                    goneUnless="@{demographic.tribe==1}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/akan_specify"/>

                <Spinner
                    goneUnless="@{demographic.tribe==1}"
                    android:id="@+id/akan"
                    selection="@{demographic.akan_tribe}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:minHeight="48dp"
                    android:onItemSelected="@{demographic::setAkan_tribe}"
                    android:padding="15dp"
                    android:spinnerMode="dialog"/>

                <com.google.android.material.textfield.TextInputLayout
                    goneUnless="@{demographic.tribe==77}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/attend_you_other"
                    android:text="@string/attend_you_other">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/tribe_oth"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={demographic.tribe_oth}"
                        android:inputType="textCapCharacters"/>
                </com.google.android.material.textfield.TextInputLayout>


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/religion"/>

                <Spinner
                    android:id="@+id/religion"
                    selection="@{demographic.religion}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:minHeight="48dp"
                    android:onItemSelected="@{demographic::setReligions}"
                    android:padding="15dp"
                    android:spinnerMode="dialog"/>

                <LinearLayout
                    goneUnless="@{demographic.religion==1}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <ImageButton
                        android:id="@+id/deno_button"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:src="@android:drawable/ic_dialog_info"
                        android:backgroundTint="@color/white"
                        android:onClick="denoInfo" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:padding="5dp"
                        android:textColor="@color/black"
                        android:text="@string/denomination_specify"/>

                </LinearLayout>

                <Spinner
                    goneUnless="@{demographic.religion==1}"
                    android:id="@+id/denomination"
                    selection="@{demographic.denomination}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:minHeight="48dp"
                    android:onItemSelected="@{demographic::setDenomination}"
                    android:padding="15dp"
                    android:spinnerMode="dialog"/>

                <LinearLayout
                    goneUnless="@{demographic.religion==77}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <com.google.android.material.textfield.TextInputLayout
                        goneUnless="@{demographic.religion==77}"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:padding="5dp"
                        android:hint="@string/attend_you_other"
                        android:text="@string/attend_you_other">

                        <androidx.appcompat.widget.AppCompatEditText
                            android:id="@+id/religion_oth"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@={demographic.religion_oth}"
                            android:inputType="textCapCharacters"/>
                    </com.google.android.material.textfield.TextInputLayout>

                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/education"/>

                <Spinner
                    android:id="@+id/education"
                    selection="@{demographic.education}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:minHeight="48dp"
                    android:onItemSelected="@{demographic::setEducations}"
                    android:padding="15dp"
                    android:spinnerMode="dialog" />

                <com.google.android.material.textfield.TextInputLayout
                    goneUnless="@{demographic.education!=1 &amp; demographic.education!=6}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/comp_yrs"
                    android:text="@string/comp_yrs">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/comp_yrs"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={demographic.comp_yrs}"
                        android:inputType="number"/>
                </com.google.android.material.textfield.TextInputLayout>

                <TextView
                    android:visibility="gone"
                    android:id="@+id/yn_occupation"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/occupations"/>

                <org.openhds.hdsscapture.SearchableSpinner
                    android:visibility="gone"
                    android:id="@+id/occupation"
                    selection="@{demographic.occupation}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:minHeight="48dp"
                    android:onItemSelected="@{demographic::setOccupations}"
                    android:padding="15dp"
                    android:spinnerMode="dialog"
                    android:textColor="#000301" />

                <com.google.android.material.textfield.TextInputLayout
                    goneUnless="@{demographic.occupation==77}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/attend_you_other"
                    android:text="@string/attend_you_other">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/occupation_oth"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={demographic.occupation_oth}"
                        android:inputType="textCapCharacters"/>
                </com.google.android.material.textfield.TextInputLayout>

                <TextView
                    android:visibility="gone"
                    android:id="@+id/yn_marital"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/marital"/>

                <Spinner
                    android:visibility="gone"
                    android:id="@+id/marital"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    selection="@{demographic.marital}"
                    android:onItemSelected="@{demographic::setMaritals}"
                    android:gravity="left"
                    android:spinnerMode="dialog"
                    android:padding="15dp"/>


            </LinearLayout>

        </ScrollView>

        <LinearLayout
            android:id="@+id/linearLayout_save"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">


            <Button
                android:id="@+id/buttonSaveClose"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_weight="1"
                style="@style/SupervisorButton"
                android:text="@string/savecontinuedata" />

            <Button
                android:id="@+id/buttonClose"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_weight="1"
                style="@style/CloseButton"
                android:text="@string/closedata" />


        </LinearLayout>

    </LinearLayout>

</layout>