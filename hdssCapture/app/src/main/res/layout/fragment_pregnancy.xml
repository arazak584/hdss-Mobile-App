<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:bind="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="pregnancy"
            type="org.openhds.hdsscapture.entity.Pregnancy" />

        <variable
            name="preg"
            type="org.openhds.hdsscapture.entity.Pregnancy" />



        <variable
            name="eventname"
            type="String" />


    </data>

    <LinearLayout
        android:id="@+id/PREGNANCY_LAYOUT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="5dp"
        tools:context=".fragment.PregnancyFragment">

        <TextView
            android:id="@+id/preg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="10dp"
            android:textAllCaps="true"
            android:text="@string/pregobs"
            android:textColor="@color/colorPrimary"
            android:textSize="30sp"
            android:textStyle="bold" />


        <TextView
            android:id="@+id/ind"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="30sp"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:textAlignment="center"/>

        <TextView
            android:visibility="gone"
            android:id="@+id/txt_comment"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textStyle="bold"
            android:text="@={pregnancy.comment}"
            android:textColor="@color/purple_700"
            android:gravity="start"/>

        <View
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_margin="5dp"
            android:background="@color/colorPrimaryDark" />


        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/earliest"
                    android:text="@string/earliest">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:id="@+id/earliest"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>


                <com.google.android.material.textfield.TextInputLayout
                    android:visibility="visible"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/lastpreg"
                    android:text="@string/lastpreg">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/last_preg"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={preg.recordedDate}"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/uuid"
                    android:text="@string/uuid">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/uuid_preg"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={preg.uuid}"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/fw"
                    android:text="@string/fw">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/fw"
                        android:text="@={pregnancy.fw_uuid}"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>

                <Button
                    android:visibility="gone"
                    android:id="@+id/button_preg_insertDate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/visitDate"
                    tools:ignore="DuplicateSpeakableTextCheck"
                    app:icon="@drawable/ic_baseline_calendar_month_24" />

                <TextView
                    android:visibility="gone"
                    android:id="@+id/pregnancy_insertDate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:autofillHints=""
                    android:ems="10"
                    android:minHeight="40dp"
                    android:text="@={pregnancy.insertDate}"
                    android:textSize="20sp"
                    tools:ignore="TextContrastCheck" />

                <com.google.android.material.textfield.TextInputLayout
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/uuid"
                    android:text="@string/uuid">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/uuid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={pregnancy.uuid}"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>


                <com.google.android.material.textfield.TextInputLayout
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/ind_uuid"
                    android:text="@string/ind_uuid">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/extid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={pregnancy.individual_uuid}"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>

                <com.google.android.material.textfield.TextInputLayout
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/visitid"
                    android:text="@string/visitid">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/preg_visitid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={pregnancy.visit_uuid}"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>

                <Button
                    android:id="@+id/button_preg_startDate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/conception"
                    tools:ignore="DuplicateSpeakableTextCheck"
                    android:background="@drawable/btnd_save"
                    android:textColor="@android:color/background_light"
                    app:icon="@drawable/ic_baseline_calendar_month_24" />

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/conception"
                    android:text="@string/conception">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:focusable="false"
                        android:focusableInTouchMode="false"
                        android:id="@+id/editText_recordedDate"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={pregnancy.recordedDate}"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>

                <Button
                    android:visibility="gone"
                    android:id="@+id/button_preg_expectDate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/expectedDelivery"
                    tools:ignore="DuplicateSpeakableTextCheck"
                    app:icon="@drawable/ic_baseline_calendar_month_24" />

                <com.google.android.material.textfield.TextInputLayout
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/expectedDelivery"
                    android:text="@string/expectedDelivery">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/expectedDelivery"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={pregnancy.expectedDeliveryDate}"
                        android:inputType="textPersonName"/>
                </com.google.android.material.textfield.TextInputLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/anteNatalClinic"/>

                <org.openhds.hdsscapture.SearchableSpinner
                    android:id="@+id/anteNatalClinic"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    selection="@{pregnancy.anteNatalClinic}"
                    android:onItemSelected="@{pregnancy::setAnteNatalClinic}"
                    android:gravity="left"
                    android:spinnerMode="dialog"
                    android:padding="15dp"/>

                <TextView
                    goneUnless="@{pregnancy.anteNatalClinic==2}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/why_no"/>

                <org.openhds.hdsscapture.SearchableSpinner
                    goneUnless="@{pregnancy.anteNatalClinic==2}"
                    android:id="@+id/why_no"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    selection="@{pregnancy.why_no}"
                    android:onItemSelected="@{pregnancy::setWhy_no}"
                    android:gravity="left"
                    android:spinnerMode="dialog"
                    android:padding="15dp"/>

                <com.google.android.material.textfield.TextInputLayout
                    goneUnless="@{pregnancy.why_no==77}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/attend_you_other"
                    android:text="@string/attend_you_other">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/why_no_other"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={pregnancy.why_no_other}"
                        android:inputType="textPersonName|textCapCharacters"/>
                </com.google.android.material.textfield.TextInputLayout>


             <LinearLayout
                    goneUnless="@{pregnancy.anteNatalClinic==1}"
                    android:id="@+id/PREG_LAYOUT"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:padding="15dp"
                        android:text="@string/anc"
                        android:textSize="20sp"
                        android:textColor="@color/colorPrimary"
                        android:textStyle="bold" />

                 <TextView
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:textColor="@color/black"
                     android:text="@string/ageOfPregFromPregNotes" />

                 <com.google.android.material.textfield.TextInputLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:gravity="start"
                     android:padding="5dp"
                     android:text="@string/ageOfPregFromPregNotes">

                     <androidx.appcompat.widget.AppCompatEditText
                         android:id="@+id/ageOfPregFromPregNotes"
                         android:layout_width="match_parent"
                         android:layout_height="wrap_content"
                         android:text="@={pregnancy.ageOfPregFromPregNotes}"
                         android:inputType="number"/>
                 </com.google.android.material.textfield.TextInputLayout>




                 <TextView
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:textColor="@color/black"
                     android:text="@string/estimatedAgeOfPreg" />

                 <EditText
                     android:id="@+id/estimatedAgeOfPreg"
                     android:layout_width="match_parent"
                     android:layout_height="50dp"
                     android:ems="10"
                     android:inputType="number"
                     android:focusable="false"
                     android:focusableInTouchMode="false"
                     android:text="@={pregnancy.estimatedAgeOfPreg}" />

                 <Button
                     android:id="@+id/button_lastClinicVisitDate"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:text="@string/lastClinicVisitDate"
                     tools:ignore="DuplicateSpeakableTextCheck"
                     android:background="@drawable/btnd_save"
                     android:textColor="@android:color/background_light"
                     app:icon="@drawable/ic_baseline_calendar_month_24" />

                 <EditText
                     android:id="@+id/editText_lastClinicVisitDate"
                     android:layout_width="match_parent"
                     android:layout_height="50dp"
                     android:ems="10"
                     android:inputType="date"
                     android:focusable="false"
                     android:focusableInTouchMode="false"
                     android:text="@={pregnancy.lastClinicVisitDate}" />


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:padding="5dp"
                        android:textColor="@color/black"
                        android:text="@string/attend_you"/>

                    <org.openhds.hdsscapture.SearchableSpinner
                        android:id="@+id/attend_you"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        selection="@{pregnancy.attend_you}"
                        android:onItemSelected="@{pregnancy::setAttend_you}"
                        android:gravity="left"
                        android:spinnerMode="dialog"
                        android:padding="20dp"/>

                 <com.google.android.material.textfield.TextInputLayout
                     goneUnless="@{pregnancy.attend_you==77}"
                      android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:gravity="start"
                     android:padding="5dp"
                     android:hint="@string/attend_you_other"
                     android:text="@string/attend_you_other">

                     <androidx.appcompat.widget.AppCompatEditText
                         android:id="@+id/attend_you_other"
                         android:layout_width="match_parent"
                         android:layout_height="wrap_content"
                         android:text="@={pregnancy.attend_you_other}"
                         android:inputType="textPersonName|textCapCharacters"/>
                 </com.google.android.material.textfield.TextInputLayout>

                 <TextView
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:gravity="start"
                     android:padding="5dp"
                     android:textColor="@color/black"
                     android:text="@string/ttinjection"/>

                 <org.openhds.hdsscapture.SearchableSpinner
                     android:id="@+id/ttinjection"
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     selection="@{pregnancy.ttinjection}"
                     android:onItemSelected="@{pregnancy::setTtinjection}"
                     android:gravity="left"
                     android:spinnerMode="dialog"
                     android:padding="20dp"/>


                 <TextView
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:textColor="@color/black"
                     android:text="@string/first_rec" />

                 <com.google.android.material.textfield.TextInputLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:gravity="start"
                     android:padding="5dp"
                     android:text="@string/first_rec">

                     <androidx.appcompat.widget.AppCompatEditText
                         android:id="@+id/first_rec"
                         android:layout_width="match_parent"
                         android:layout_height="wrap_content"
                         android:text="@={pregnancy.first_rec}"
                         android:inputType="number"/>
                 </com.google.android.material.textfield.TextInputLayout>


                 <TextView
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:textColor="@color/black"
                     android:text="@string/anc_visits" />

                 <com.google.android.material.textfield.TextInputLayout
                     android:layout_width="match_parent"
                     android:layout_height="wrap_content"
                     android:gravity="start"
                     android:padding="5dp"
                     android:text="@string/first_rec">

                     <androidx.appcompat.widget.AppCompatEditText
                         android:id="@+id/anc_visits"
                         android:layout_width="match_parent"
                         android:layout_height="wrap_content"
                         android:text="@={pregnancy.anc_visits}"
                         android:inputType="number"/>
                 </com.google.android.material.textfield.TextInputLayout>

                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/own_bnet"/>

                <org.openhds.hdsscapture.SearchableSpinner
                    android:id="@+id/own_bnet"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    selection="@{pregnancy.own_bnet}"
                    android:onItemSelected="@{pregnancy::setOwn_bnet}"
                    android:gravity="left"
                    android:spinnerMode="dialog"
                    android:padding="15dp"/>

                <LinearLayout
                    goneUnless="@{pregnancy.own_bnet==1}"
                    android:id="@+id/BED_LAYOUT"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:padding="15dp"
                        android:text="@string/bednet"
                        android:textSize="20sp"
                        android:textColor="@color/colorPrimary"
                        android:textStyle="bold" />

                    <com.google.android.material.textfield.TextInputLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:padding="5dp"
                        android:hint="@string/how_many"
                        android:text="@string/how_many">

                        <androidx.appcompat.widget.AppCompatEditText
                            android:id="@+id/how_many"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@={pregnancy.how_many}"
                            android:inputType="number"/>
                    </com.google.android.material.textfield.TextInputLayout>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:padding="5dp"
                        android:textColor="@color/black"
                        android:text="@string/bnet_sou"/>

                    <org.openhds.hdsscapture.SearchableSpinner
                        android:id="@+id/bnet_sou"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        selection="@{pregnancy.bnet_sou}"
                        android:onItemSelected="@{pregnancy::setBnet_sou}"
                        android:gravity="left"
                        android:spinnerMode="dialog"
                        android:padding="20dp"/>

                    <com.google.android.material.textfield.TextInputLayout
                        goneUnless="@{pregnancy.bnet_sou==77}"
                            android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:padding="5dp"
                        android:hint="@string/attend_you_other"
                        android:text="@string/attend_you_other">

                        <androidx.appcompat.widget.AppCompatEditText
                            android:id="@+id/bnet_sou_other"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@={pregnancy.bnet_sou_other}"
                            android:inputType="textPersonName|textCapCharacters"/>
                    </com.google.android.material.textfield.TextInputLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:padding="5dp"
                        android:textColor="@color/black"
                        android:text="@string/bnet_loc"/>

                    <org.openhds.hdsscapture.SearchableSpinner
                        android:id="@+id/bnet_loc"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        selection="@{pregnancy.bnet_loc}"
                        android:onItemSelected="@{pregnancy::setBnet_loc}"
                        android:gravity="left"
                        android:spinnerMode="dialog"
                        android:padding="20dp"/>


                    <com.google.android.material.textfield.TextInputLayout
                        goneUnless="@{pregnancy.bnet_loc==77}"
                            android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:padding="5dp"
                        android:hint="@string/attend_you_other"
                        android:text="@string/attend_you_other">

                        <androidx.appcompat.widget.AppCompatEditText
                            android:id="@+id/bnet_loc_other"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="@={pregnancy.bnet_loc_other}"
                            android:inputType="textCapCharacters"/>
                    </com.google.android.material.textfield.TextInputLayout>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:padding="5dp"
                        android:textColor="@color/black"
                        android:text="@string/slp_bednet"/>

                    <org.openhds.hdsscapture.SearchableSpinner
                        android:id="@+id/slp_bednet"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        selection="@{pregnancy.slp_bednet}"
                        android:onItemSelected="@{pregnancy::setSlp_bednet}"
                        android:gravity="left"
                        android:spinnerMode="dialog"
                        android:padding="20dp"/>

                    <TextView
                        goneUnless="@{pregnancy.slp_bednet==1}"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="start"
                        android:padding="5dp"
                        android:textColor="@color/black"
                        android:text="@string/trt_bednet"/>

                    <org.openhds.hdsscapture.SearchableSpinner
                        goneUnless="@{pregnancy.slp_bednet==1}"
                        android:id="@+id/trt_bednet"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        selection="@{pregnancy.trt_bednet}"
                        android:onItemSelected="@{pregnancy::setTrt_bednet}"
                        android:gravity="left"
                        android:spinnerMode="dialog"
                        android:padding="20dp"/>

                </LinearLayout>


                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/healthfacility"/>

                <org.openhds.hdsscapture.SearchableSpinner
                    android:id="@+id/healthfacility"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    selection="@{pregnancy.healthfacility}"
                    android:onItemSelected="@{pregnancy::setHealthfacility}"
                    android:gravity="left"
                    android:spinnerMode="dialog"
                    android:padding="20dp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/medicineforpregnancy"/>

                <org.openhds.hdsscapture.SearchableSpinner
                    android:id="@+id/medicineforpregnancy"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    selection="@{pregnancy.medicineforpregnancy}"
                    android:onItemSelected="@{pregnancy::setMedicineforpregnancy}"
                    android:gravity="left"
                    android:spinnerMode="dialog"
                    android:padding="20dp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/first_preg"/>

                <org.openhds.hdsscapture.SearchableSpinner
                    android:id="@+id/first_preg"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    selection="@{pregnancy.first_preg}"
                    android:onItemSelected="@{pregnancy::setFirst_preg}"
                    android:gravity="left"
                    android:spinnerMode="dialog"
                    android:padding="20dp"/>

                <com.google.android.material.textfield.TextInputLayout
                    goneUnless="@{pregnancy.first_preg==2}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:hint="@string/pregnancyNumber"
                    android:text="@string/pregnancyNumber">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/pregnancyNumber"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="@={pregnancy.pregnancyNumber}"
                        android:inputType="number"/>
                </com.google.android.material.textfield.TextInputLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:text="@string/family_planning" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/preg_ready"/>

                <org.openhds.hdsscapture.SearchableSpinner
                    android:id="@+id/preg_ready"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    selection="@{pregnancy.preg_ready}"
                    android:onItemSelected="@{pregnancy::setPreg_ready}"
                    android:gravity="left"
                    android:spinnerMode="dialog"
                    android:padding="15dp"/>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/family_plan"/>

                <org.openhds.hdsscapture.SearchableSpinner
                    android:id="@+id/family_plan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    selection="@{pregnancy.family_plan}"
                    android:onItemSelected="@{pregnancy::setFamily_plan}"
                    android:gravity="left"
                    android:spinnerMode="dialog"
                    android:padding="15dp"/>

                <TextView
                    goneUnless="@{pregnancy.family_plan==1}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/plan_method"/>

                <org.openhds.hdsscapture.SearchableSpinner
                    goneUnless="@{pregnancy.family_plan==1}"
                    android:id="@+id/plan_method"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    selection="@{pregnancy.plan_method}"
                    android:onItemSelected="@{pregnancy::setPlan_method}"
                    android:gravity="left"
                    android:spinnerMode="dialog"
                    android:padding="15dp"/>

                <com.google.android.material.textfield.TextInputLayout
                    goneUnless="@{pregnancy.plan_method==77}"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:hint="@string/plan_method_oth"
                    android:padding="5dp"
                    android:text="@string/plan_method_oth">

                    <androidx.appcompat.widget.AppCompatEditText
                        android:id="@+id/plan_method_oth"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="textPersonName"
                        android:text="@={pregnancy.plan_method_oth}" />
                </com.google.android.material.textfield.TextInputLayout>
                

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/outcome"/>

                <org.openhds.hdsscapture.SearchableSpinner
                    android:id="@+id/outcometype"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    selection="@{pregnancy.outcome}"
                    android:onItemSelected="@{pregnancy::setOutcome}"
                    android:gravity="left"
                    android:spinnerMode="dialog"
                    android:padding="10dp"/>

                <Button
                    goneUnless="@{pregnancy.outcome==1}"
                    android:id="@+id/button_pregnancy_outcome"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/outcomeDate"
                    tools:ignore="DuplicateSpeakableTextCheck"
                    android:background="@drawable/btnd_save"
                    android:textColor="@android:color/background_light"
                    app:icon="@drawable/ic_baseline_calendar_month_24" />

                <TextView
                    goneUnless="@{pregnancy.outcome==1}"
                    android:id="@+id/editText_outcomeDate"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:autofillHints=""
                    android:ems="10"
                    android:minHeight="40dp"
                    android:text="@={pregnancy.outcome_date}"
                    android:textSize="20sp"
                    tools:ignore="TextContrastCheck" />

                <TextView
                    goneUnless="@{pregnancy.outcome==1}"
                    android:id="@+id/ext"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/extra"/>

                <org.openhds.hdsscapture.SearchableSpinner
                    goneUnless="@{pregnancy.outcome==1}"
                    android:id="@+id/extra"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    selection="@{pregnancy.extra}"
                    android:onItemSelected="@{pregnancy::setExtra}"
                    android:gravity="left"
                    android:spinnerMode="dialog"
                    android:padding="15dp"/>

                <TextView
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="start"
                    android:padding="5dp"
                    android:textColor="@color/black"
                    android:text="@string/form_complete"/>

                <org.openhds.hdsscapture.SearchableSpinner
                    android:visibility="gone"
                    android:id="@+id/individual_complete"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    selection="@{pregnancy.complete}"
                    android:onItemSelected="@{pregnancy::setComplete}"
                    android:gravity="left"
                    android:spinnerMode="dialog"
                    android:padding="15dp"/>

                <TextView
                    android:visibility="gone"
                    android:id="@+id/resolve"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:text="@string/query_reslve" />

                <RadioGroup
                    android:visibility="gone"
                    android:id="@+id/status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:onCheckedChanged="@{pregnancy::setStatus}"
                    android:orientation="horizontal">

                    <RadioButton
                        android:id="@+id/no_3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:checked="@{pregnancy.status==3}"
                        android:tag="3"
                        android:text="@string/yes" />

                    <RadioButton
                        android:id="@+id/no_2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:checked="@{pregnancy.status==2}"
                        android:tag="2"
                        android:text="@string/no" />

                    <RadioButton
                        android:visibility="invisible"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:checked="@{pregnancy.status==0}"
                        android:tag="0"/>
                </RadioGroup>


            </LinearLayout>
            </LinearLayout>

        </ScrollView>
        <LinearLayout
            android:id="@+id/linearLayout_save"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">


            <Button
                android:id="@+id/buttonSaveClose"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/btnd_save"
                android:textColor="@android:color/background_light"
                android:text="@string/saveclosedata" />

            <Button
                android:id="@+id/buttonClose"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/btnd_close"
                android:textColor="@android:color/background_light"
                android:text="@string/closedata" />

        </LinearLayout>

    </LinearLayout>


</layout>