<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="pregoutcome2"
            type="org.openhds.hdsscapture.entity.Outcome" />

    </data>

    <LinearLayout
        android:id="@+id/OUTCOME_LAYOUT"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="10dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:padding="10dp"
            android:text="Child-2"
            android:textAllCaps="true"
            android:textColor="@color/colorPrimary"
            android:textSize="30sp"
            android:textStyle="bold" />

        <com.google.android.material.textfield.TextInputLayout
            android:visibility="gone"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/uuid"
            android:text="@string/uuid">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/out2_uuid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={pregoutcome2.uuid}"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textPersonName"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:visibility="gone"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/preguuid"
            android:text="@string/preguuid">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/preg2_uuid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@{pregoutcome2.preg_uuid}"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textPersonName"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:visibility="gone"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/motherid"
            android:text="@string/motherid">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/out2_preguuid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={pregoutcome2.mother_uuid}"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textPersonName"/>
        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:textColor="@color/black"
            android:text="@string/pregoutcome"/>

        <Spinner
            android:id="@+id/out2_type"
            selection="@{pregoutcome2.type}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:minHeight="48dp"
            android:onItemSelected="@{pregoutcome2::setType}"
            android:padding="15dp"
            android:spinnerMode="dialog" />

        <com.google.android.material.textfield.TextInputLayout
            android:visibility="gone"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/childextid"
            android:text="@string/childextid">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/out2_child_extid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={pregoutcome2.childuuid}"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textPersonName"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            goneUnless="@{pregoutcome2.type==1}"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:inputType="text"
            android:hint="@string/ind_extid"
            android:text="@string/ind_extid">

            <com.google.android.material.textfield.TextInputEditText
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:id="@+id/individual1_extid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={pregoutcome2.extId}"
                android:inputType="textPersonName"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:visibility="gone"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/ind_uuid"
            android:text="@string/ind_uuid">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/individual1_mother"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={pregoutcome2.individual_uuid}"
                android:focusable="false"
                android:focusableInTouchMode="false"
                android:inputType="textPersonName|textCapCharacters"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            goneUnless="@{pregoutcome2.type==1}"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/firstname"
            android:text="@string/firstname">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/individual1_firstName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={pregoutcome2.firstName}"
                android:required="true"
                android:inputType="textPersonName|textCapCharacters"/>
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            goneUnless="@{pregoutcome2.type==1}"
            style="@style/Widget.MaterialComponents.TextInputLayout.FilledBox.Dense"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:hint="@string/lastname"
            android:text="@string/lastname">

            <com.google.android.material.textfield.TextInputEditText
                android:id="@+id/individual1_lastName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@={pregoutcome2.lastName}"
                android:required="true"
                android:inputType="textPersonName|textCapCharacters"/>
        </com.google.android.material.textfield.TextInputLayout>

        <TextView
            goneUnless="@{pregoutcome2.type==1}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:textColor="@color/black"
            android:text="@string/gender"/>

        <Spinner
            android:id="@+id/gender"
            goneUnless="@{pregoutcome2.type==1}"
            selection="@{pregoutcome2.gender}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:minHeight="48dp"
            android:onItemSelected="@{pregoutcome2::setGenders}"
            android:padding="10dp"
            android:required="true"
            android:spinnerMode="dialog" />

        <TextView
            goneUnless="@{pregoutcome2.type==1}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:textColor="@color/black"
            android:text="@string/rltn_head"/>

        <Spinner
            android:id="@+id/rltn_head"
            goneUnless="@{pregoutcome2.type==1}"
            selection="@{pregoutcome2.rltn_head}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:minHeight="48dp"
            android:onItemSelected="@{pregoutcome2::setRltn_head}"
            android:padding="10dp"
            android:spinnerMode="dialog" />

        <TextView
            goneUnless="@{pregoutcome2.type==1}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:textColor="@color/black"
            android:text="@string/chd_weight"/>

        <Spinner
            goneUnless="@{pregoutcome2.type==1}"
            android:id="@+id/chd_weight"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:spinnerMode="dialog"
            selection="@{pregoutcome2.chd_weight}"
            android:onItemSelected="@{pregoutcome2::setChd_weight}"
            android:padding="10dp"/>

        <TextView
            goneUnless="@{pregoutcome2.chd_weight==1}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="@string/weig_hcard" />

        <EditText
            goneUnless="@{pregoutcome2.chd_weight==1}"
            android:id="@+id/weig_hcard"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:ems="10"
            android:inputType="numberDecimal"
            android:text="@={pregoutcome2.weig_hcard}" />

        <TextView
            goneUnless="@{pregoutcome2.type==1}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="start"
            android:padding="5dp"
            android:textColor="@color/black"
            android:text="@string/chd_size"/>

        <Spinner
            goneUnless="@{pregoutcome2.type==1}"
            android:id="@+id/chd_size"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:spinnerMode="dialog"
            selection="@{pregoutcome2.chd_size}"
            android:onItemSelected="@{pregoutcome2::setChd_size}"
            android:padding="10dp"/>

    </LinearLayout>

</layout>